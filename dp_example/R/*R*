> emsf.experiment.generate.P <- function(n, alpha.dir, na = 1)
+ {
+   P <- array(0, c(n, n, na))
+   for (a in 1:na) P[,,a] <- rdirichlet(n,rep(alpha.dir, n))
+   P 
+ }

