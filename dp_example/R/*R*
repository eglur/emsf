> emsf.experiment.plot.results.by.tc <- function(save=FALSE)
+ {
+     tcs <- seq(100, 1000, by = 100)
+ 
+     for (tc in tcs) {
+         emsf.experiment.plot.results(tcs=c(tc), inds=1:4, save=TRUE)
+     }
+ }

